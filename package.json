{
  "name": "bpmn-js-cli",
  "version": "2.4.0",
  "description": "A command-line interface for bpmn-js",
  "scripts": {
    "all": "run-s lint test",
    "lint": "eslint .",
    "start": "SINGLE_START=1 npm run dev",
    "dev": "npm test -- --auto-watch --no-single-run",
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bpmn-io/bpmn-js-cli"
  },
  "keywords": [
