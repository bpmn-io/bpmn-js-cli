  ],
  "author": "Nico Rehwaldt<git_nikku@nixis.de>",
  "license": "MIT",
  "devDependencies": {
    "bpmn-js": "^18.0.0",
    "chai": "^6.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-bpmn-io": "^2.0.1",
    "karma": "^6.4.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-debug-launcher": "^0.0.5",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^2.1.3",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.1",
    "mocha": "^11.0.0",
    "mocha-test-container-support": "^0.2.0",
    "npm-run-all2": "^8.0.0",
    "puppeteer": "^24.34.0",
    "semver": "^7.7.3",
    "webpack": "^5.104.1"
  },
  "dependencies": {
    "min-dash": "^5.0.0"
  },
